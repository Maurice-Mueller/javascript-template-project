{
  "name": "javascript_template_project",
  "version": "1.0.0",
  "description": "a simple javascript template project",
  "main": "index.js",
  "scripts": {
    "share-by-localtunnel": "lt --port 8088",
    "security-check": "nsp check",
    "lint": "esw webpack src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel security-check start-dev-server lint:watch test:watch start-mock-api",
    "start-dev-server": "babel-node buildScripts/srcServer.js",
    "prestart-dev-server": "babel-node buildScripts/startMessageSrcServer.js",
    "test": "mocha --reporter progress buildScripts/testSetup.js 'src/**/*.test.js' --color",
    "test:watch": "watch-run -p 'src/**' npm run test",
    "generate-mock-data": "babel-node buildScripts/mock/generateMockData",
    "start-mock-api": "json-server --watch src/api/mockSchema.json --port 8090 --routes buildScripts/mock/jsonServerRoutes.json",
    "build": "babel-node buildScripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoePad/javascript-template-project.git"
  },
  "author": "MoePad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoePad/javascript-template-project/issues"
  },
  "homepage": "https://github.com/MoePad/javascript-template-project#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.1.0",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "eslint": "4.2.0",
    "eslint-watch": "3.1.2",
    "express": "4.15.3",
    "faker": "4.1.0",
    "jsdom": "11.1.0",
    "json-schema-faker": "0.5.0-rc10",
    "json-server": "0.11.2",
    "localtunnel": "1.8.3",
    "mocha": "3.4.2",
    "npm-run-all": "4.0.2",
    "open": "0.0.5",
    "style-loader": "0.18.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "watch-run": "1.2.5",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.1",
    "webpack-middleware": "1.5.1",
    "whatwg-fetch": "2.0.3"
  }
}
