{
  "name": "vuejs-typescript",
  "version": "1.0.0",
  "description": "- `npm init` / `npm init -f` -",
  "main": "src/main.ts",
  "scripts": {
    "clean-mod": "rm -rf node_modules/",
    "reset-mod": "npm run clean-mod && npm cache clean --force && npm cache clear --force && npm install",
    "clean-build": "rm -rf dist/",
    "build": "npm run clean-build && ts-node config/scripts/build.ts",
    "dev": "ts-node config/scripts/startDevServer.ts",
    "prod": "npm run build && ts-node config/scripts/startProdServer.ts",
    "test-karma": "karma start config/karma/karma.conf.js -v",
    "test-karma2": "karma start karma.conf.js --debug",
    "test2": "mocha-webpack --reporter progress --require ts-node/register --webpack-config config/webpack/webpack.test.conf.ts src/test/vue-for-test.ktest.ts",
    "test": "mocha-webpack --reporter progress --require ts-node/register --webpack-config config/webpack/webpack.test.conf.ts src/test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoePad/javascript-template-project.git"
  },
  "author": "Moe Pad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoePad/javascript-template-project/issues"
  },
  "homepage": "https://github.com/MoePad/javascript-template-project#readme",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chalk": "^0.4.31",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.36",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jsdom": "^11.0.1",
    "@types/karma": "^0.13.36",
    "@types/mocha": "^2.2.41",
    "@types/open": "0.0.29",
    "@types/vue": "^2.0.0",
    "@types/webpack": "^3.0.5",
    "@types/webpack-merge": "0.0.5",
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "chrome-headless-launcher": "^0.1.5",
    "compression": "^1.7.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.1.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.4",
    "karma-typescript-preprocessor": "^0.3.1",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.7",
    "mocha-webpack": "^0.7.0",
    "open": "0.0.5",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.14",
    "ts-loader": "^2.3.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "vue": "^2.4.2",
    "vue-class-component": "^5.0.2",
